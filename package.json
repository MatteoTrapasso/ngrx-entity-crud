{
  "name": "ngrx-entity-crud",
  "version": "11.6.0-beta.8",
  "repository": "https://github.com/jucasoft/ngrx-entity-crud",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ngrx-entity-crud && npm run build:schematics",
    "testLibs": "ng test ngrx-entity-crud",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:schematics": "tsc -p ./projects/ngrx-entity-crud/tsconfig.schematics.json",
    "copy:schemas": "rsync -R ./projects/ngrx-entity-crud/schematics/*/schema.json ./libs/ngrx-entity-crud/",
    "copy:files": "rsync -Ravz ./projects/ngrx-entity-crud/schematics/*/files/** ./libs/ngrx-entity-crud/",
    "copy:collection": "cp ./projects/ngrx-entity-crud/schematics/collection.json ./libs/ngrx-entity-crud/schematics/collection.json",
    "merge": "rsync -a ./libs/ngrx-entity-crud/projects/ngrx-entity-crud/schematics/ ./libs/ngrx-entity-crud/schematics/",
    "remove": "rm -rf ./libs/ngrx-entity-crud/projects",
    "postbuild": "npm run copy:schemas && npm run copy:files && npm run copy:collection && npm run merge && npm run remove"
  },
  "dependencies": {
    "@angular/animations": "~11.0.0",
    "@angular/cdk": "^11.0.0",
    "@angular/common": "~11.0.0",
    "@angular/compiler": "~11.0.0",
    "@angular/core": "~11.0.0",
    "@angular/forms": "~11.0.0",
    "@angular/platform-browser": "~11.0.0",
    "@angular/platform-browser-dynamic": "~11.0.0",
    "@angular/router": "~11.0.0",
    "@ngrx/effects": "^10.0.1",
    "@ngrx/entity": "^10.0.1",
    "@ngrx/router-store": "^10.0.1",
    "@ngrx/store": "^10.0.1",
    "@ngrx/store-devtools": "^10.0.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2",
    "deepmerge": "^4.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.1",
    "@angular-devkit/core": "^11.0.1",
    "@angular-devkit/schematics": "^11.0.1",
    "@angular-devkit/schematics-cli": "^0.1000.5",
    "@angular/cli": "~11.0.1",
    "@angular/compiler-cli": "~11.0.0",
    "@angular/language-service": "~11.0.0",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.0.12",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.2",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  }
}
